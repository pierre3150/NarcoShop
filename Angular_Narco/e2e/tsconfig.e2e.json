{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist-e2e",
    "rootDir": "./",
    "declaration": false,
    "resolveJsonModule": true,
    "types": ["node", "jest", "selenium-webdriver"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist-e2e"]
}

